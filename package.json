{
  "name": "jimple",
  "version": "1.3.0",
  "description": "A port of Pimple Dependency Injection container to NodeJS and to the browser developed in ES6",
  "main": "src/Jimple.js",
  "tonicExampleFilename": "tonicExample.js",
  "scripts": {
    "build-for-browser": "babel -d dist src/",
    "test-in-browser": "karma start karma.config.js",
    "test-in-node": "mocha -R spec",
    "test": "npm run-script test-in-node && npm run-script test-in-browser"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fjorgemota/jimple.git"
  },
  "keywords": [
    "jimple",
    "pimple",
    "dependency",
    "injection",
    "di",
    "container",
    "dependency injection",
    "services",
    "parameters",
    "inversion of control",
    "IoC"
  ],
  "author": "Fernando Jorge Mota <contato@fjorgemota.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjorgemota/jimple/issues"
  },
  "homepage": "https://github.com/fjorgemota/jimple#readme",
  "devDependencies": {
    "babel-cli": "^6.6.0",
    "babel-core": "^6.6.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.0",
    "babel-polyfill": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "expect.js": "^0.3.1",
    "karma": "^0.13.21",
    "karma-browserify": "^5.0.2",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.24",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.4",
    "watchify": "^3.7.0"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
